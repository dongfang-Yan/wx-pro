<view class="datalist">
  <view class="list-item" wx:for="{{datalist}}" wx:key="index">
    <view class="item-img fl">
      <image class="auto-img" src="{{item.src}}"></image>
    </view>
    <view class="item-title fl">
      <view class="title">{{item.title}}</view>
      <view class="time">{{item.date}}</view>
    </view>

    <view class="item-price fl">ï¿¥{{item.money}}</view>
  </view>
</view>